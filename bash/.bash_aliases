#
# ~/.bash_aliases
#

# Aliases
alias ls='ls --color=auto'
#alias ls='ls -G'
alias ll="ls -l"
alias la="ls -la"
alias c="clear"

# Git Short Commands (see .gitconfig for git aliases)
alias gfshow="git fetch origin && echo 'fetched origin' && git remote show origin"
alias cstat="clear && git status"

